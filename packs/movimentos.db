ccess":{"key":"data.moveResults.success.value","label":"Sucesso!","value":""},"partial":{"key":"data.moveResults.partial.value","label":"Sucesso Parcial.","value":""},"failure":{"key":"data.moveResults.failure.value","label":"Falha...","value":""}},"playbook":"","rollType":"","rollMod":0,"choices":""},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"xcStorMY7ZK26Z7S":3},"flags":{"cf":{"id":"temp_qu2d18hy21c","path":"Movimentos#/CF_SEP/Cartilhas#/CF_SEP/Profissional","color":"#000000"}},"_id":"MbUg3fh5Wgs0lwJr"}
